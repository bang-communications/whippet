#!/usr/bin/env php
<?php

require file_exists(__DIR__.'/../vendor/autoload.php') ? __DIR__.'/../vendor/autoload.php' : __DIR__.'/../../../autoload.php';

define('WHIPPET_ROOT', dirname(dirname(__FILE__)));

require WHIPPET_ROOT . "/lib/whippet.class.php";

$whippet = new Whippet();
$whippet->start($argv);
